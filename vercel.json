{
  "rewrites": [
    {
      "source": "/((?!api|assets|equilibrium|.*\\..*$).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/equilibrium-cron",
      "schedule": "0 22,4,10,16 * * *"
    }
  ]
}